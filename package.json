{
  "name": "click-people",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "dev": "npm run dev --workspaces",
    "build": "npm run build --workspaces",
    "check-types": "npm run check-types --workspaces",
    "dev:native": "npm run dev --workspace native",
    "dev:web": "npm run dev --workspace web",
    "db:push": "npm run db:push --workspace @click-people/db",
    "db:studio": "npm run db:studio --workspace @click-people/db",
    "db:generate": "npm run db:generate --workspace @click-people/db",
    "db:migrate": "npm run db:migrate --workspace @click-people/db",
    "db:seed": "npm run db:seed --workspace @click-people/db"
  },
  "dependencies": {
    "@click-people/env": "*",
    "dotenv": "^17.2.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@click-people/config": "*",
    "@types/node": "^22.13.14",
    "typescript": "^5",
    "xlsx": "^0.18.5"
  },
  "packageManager": "npm@10.8.1"
}
